    <%= f.label :resumen %><br>
    <%= f.text_field :resumen %>
  </div>
  <div class="field">
    <%= f.label :descripcion %><br>
    <%= f.text_area :descripcion %>
  </div>
  <div class="field">
    <%= f.label :hora_ingreso %><br>
    <%= f.datetime_select :hora_ingreso %>
  </div>
  <div class="field">
    <%= f.label :hora_atencion %><br>
    <%= f.datetime_select :hora_atencion %>
  </div>
  <div class="field">
    <%= f.label :hora_finalizacion %><br>
    <%= f.datetime_select :hora_finalizacion %>
  </div>
  <div class="field">
    <%= f.label :usuario_id %><br>
