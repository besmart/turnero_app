    <%= f.label :comentario_atencion %><br>
    <%= f.text_area :comentario_atencion %>
  </div>
  <div class="field">
    <%= f.label :estado %><br>
    <%= f.number_field :estado %>
  </div>
  <div class="field">
    <%= f.label :agencia_id %><br>
